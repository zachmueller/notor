{
	"name": "notor",
	"version": "1.0.0",
	"description": "AI-powered assistant for Obsidian.",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint .",
		"e2e": "npx playwright test --config=e2e/playwright.config.ts",
		"e2e:run": "npx tsx e2e/run-and-collect.ts",
		"e2e:run:quick": "npx tsx e2e/run-and-collect.ts --duration 10",
		"e2e:setup-vault": "npx tsx e2e/scripts/setup-vault.ts"
	},
	"keywords": [],
	"license": "0-BSD",
	"devDependencies": {
		"@types/node": "^20.11.0",
		"@playwright/test": "^1.48.0",
		"playwright-core": "^1.48.0",
		"tsx": "^4.19.0",
		"esbuild": "0.25.5",
		"eslint-plugin-obsidianmd": "0.1.9",
		"globals": "14.0.0",
		"tslib": "2.4.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "8.35.1",
		"@eslint/js": "9.30.1",
		"jiti": "2.6.1"
	},
	"dependencies": {
		"obsidian": "latest"
	}
}
